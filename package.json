    "@web3-react/core": "^8.0.30-beta.0",
    "@web3-react/metamask": "^8.0.24-beta.0",
    "@web3-react/network": "^8.0.23-beta.0",
    "@web3-react/walletconnect": "^8.0.1-beta.0",
